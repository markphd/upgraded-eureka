                        var pageOverlay = $('#page-overlay');
                        var items = {
                        nav: $('#main-nav'),
                        packageSelector: $('.responsive-package-chooser'),
                        mainNavigation: $('.main-navigation')
                        }
                        function PageOverlayHandler () {
                        var target = event;
                        if(event.target.id == 'page-overlay'){
                        pageOverlay.removeClass('visible mobile-nav-open');
                        items.nav.removeClass('responsive');
                        items.mainNavigation.removeClass('toggled');
                        return;
                        };
                        if(items.mainNavigation.hasClass('toggled')){
                        return;
                        }
                        if(pageOverlay.hasClass('visible')){
                        items.nav.removeClass('responsive');
                        pageOverlay.removeClass('visible');
                        return;
                        };
                        pageOverlay.addClass('visible');
                        }
                        function chooseLanguage () {
                        pageOverlay.removeClass('visible mobile-nav-open');
                        items.nav.removeClass('responsive');
                        items.mainNavigation.removeClass('toggled');
                        $('body').removeClass('blur');
                        $('html, body').animate({
                        scrollTop: $('.tmpl-footer').offset().top
                        }, 500);
                        }
                        $('#more-apps, #close-nav').on('click', function(){
                        $('#main-nav').toggleClass('responsive');
                        PageOverlayHandler();
                        });
                        $('.mobile-nav-toggle, #close-menu').on('click', function(){
                        var mobileNavToggle = $('.mobile-nav-toggle');
                        var mainNavigation = $('.main-navigation');
                        var pageOverlay = $('#page-overlay');
                        pageOverlay.addClass('visible');
                        mainNavigation.toggleClass('toggled');
                        $('body').toggleClass('blur');
                        PageOverlayHandler();
                        if (pageOverlay.hasClass('visible')) {
                        pageOverlay.addClass('mobile-nav-open');
                        }
                        });
                        $('#page-overlay').on('click', function () {
                        PageOverlayHandler();
                        });
                        $('#mobile-change-language').on('click', function(){
                        chooseLanguage();
                        });
